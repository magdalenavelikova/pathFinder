round-image: url(\"/images/pic4.jpg\");\r\n    background-size: cover;\r\n    border-bottom-right-radius: 5rem;\r\n    border-top-left-radius: 5rem;\r\n}\r\n\r\n.info div:nth-child(3) {\r\n    background-image: url(\"/images/pic1.jpg\");\r\n    background-size: cover;\r\n    border-bottom-left-radius: 5rem;\r\n    border-top-right-radius: 5rem;\r\n}\r\n\r\n.details-main {\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr 1fr;\r\n    gap: 10px;\r\n    grid-auto-rows: minmax(100px, auto);\r\n}\r\n\r\n.details-main .gallery {\r\n    grid-column: 1/4;\r\n    grid-row: 3;\r\n}\r\n\r\n.level {\r\n    display: grid;\r\n    grid-template-columns: repeat(3, 1fr);\r\n}\r\n\r\n.route-info, .route-description, .comments {\r\n    background-color: var(--lightGray);\r\n    padding: 20px;\r\n    border-radius: 10px;\r\n}\r\n\r\n.route-info {\r\n    grid-column: 1;\r\n    grid-row: 1/3;\r\n}\r\n\r\n.route-description {\r\n    grid-column: 2/4;\r\n    grid-row: 2;\r\n}\r\n\r\n.comments {\r\n    grid-column: 1/4;\r\n    grid-row: 4;\r\n\r\n}\r\n\r\n#add-route-form {\r\n    border-radius: 10px;\r\n    background: rgba(0, 0, 0, 0.8);\r\n    padding: 2rem;\r\n}\r\n\r\n#map {\r\n    height: 500px;\r\n    width: 100%;\r\n    border-radius: 10px;\r\n}\r\n\r\n#video iframe {\r\n    width: 100%;\r\n    height: 500px;\r\n    border-radius: 10px;\r\n}\r\n\r\n\r\n/*\r\n    CSS for profile\r\n */\r\nbody {\r\n    background-color: rgba(0, 0, 0, 6);\r\n}\r\n\r\n.profile-card {\r\n    border: none;\r\n    position: relative;\r\n    overflow: hidden;\r\n    border-radius: 8px;\r\n    cursor: pointer;\r\n    min-height: 65vh;\r\n}\r\n\r\n.profile-card:before {\r\n    content: \"\";\r\n    position: absolute;\r\n    left: 0;\r\n    top: 0;\r\n    width: 4px;\r\n    height: 100%;\r\n    background-color: #bbbef5;\r\n    transform: scaleY(1);\r\n    transition: all 0.5s;\r\n    transform-origin: bottom\r\n}\r\n\r\n.profile-card:after {\r\n    content: \"\";\r\n    position: absolute;\r\n    left: 0;\r\n    top: 0;\r\n    width: 4px;\r\n    height: 100%;\r\n    background-color: #0833e3;\r\n    transform: scaleY(0);\r\n    transition: all 0.5s;\r\n    transform-origin: bottom\r\n}\r\n\r\n.profile-card:hover::after {\r\n    transform: scaleY(1)\r\n}\r\n\r\n.fonts {\r\n    font-size: 20px\r\n}\r\n\r\n.social-list {\r\n    display: flex;\r\n    list-style: none;\r\n    justify-content: center;\r\n    padding: 0\r\n}\r\n\r\n.social-list li {\r\n    padding: 10px;\r\n    color: #0833e3;\r\n    font-size: 19px\r\n}\r\n\r\n.buttons button:nth-child(1) {\r\n    border: 1px solid #0833e3 !important;\r\n    color: #ffffff;\r\n    height: 40px\r\n}\r\n\r\n.buttons button:nth-child(1):hover {\r\n    border: 1px solid #0833e3 !important;\r\n    color: #fff;\r\n    height: 40px;\r\n    background-color: #0833e3\r\n}\r\n\r\n.buttons button:nth-child(2) {\r\n    border: 1px solid #0833e3 !important;\r\n    color: #ffffff;\r\n    height: 40px\r\n}\r\n\r\n.buttons button:nth-child(2):hover {\r\n    border: 1px solid #0833e3 !important;\r\n    color: #fff;\r\n    height: 40px;\r\n    background-color: #0833e3\r\n}\r\n\r\n.main-nav form {\r\n    background: var(--primary);\r\n    display: block;\r\n    text-align: center;\r\n    margin: auto;\r\n    color: var(--dark);\r\n    padding: 0.8rem;\r\n\r\n}\r\n\r\n.main-nav input {\r\n    background: transparent;\r\n    /*!*display: block;*!*/\r\n    /*text-decoration: none;*/\r\n    border: none;\r\n    text-align: center;\r\n    /*color: var(--dark);*/\r\n    text-transform: uppercase;\r\n    font-size: 1.1rem;\r\n\r\n}\r\n\r\n.main-nav form:hover {\r\n    background: var(--dark);\r\n    color: var(--light);\r\n}\r\n
