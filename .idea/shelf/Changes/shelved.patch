<+>/* CSS Variables */\r\n:root {\r\n    --primary: #ddd;\r\n    --dark: #333;\r\n    --light: #fff;\r\n    --shadow: 0 1px 5px rgba(104, 104, 104, 0.8);\r\n    --lightGray: rgba(241, 241, 241, 0.8);\r\n}\r\n\r\n\r\nhtml {\r\n    box-sizing: border-box;\r\n    font-family: Arial, Helvetica, sans-serif;\r\n    color: var(--dark);\r\n}\r\n\r\nbody {\r\n    background-image: url(\"/images/site-bgr.jpg\");\r\n    backdrop-filter: blur(4px);\r\n    padding: 10px;\r\n    background-repeat: no-repeat;\r\n    background-attachment: fixed;\r\n    margin: 30px 50px;\r\n    line-height: 1.4;\r\n}\r\n\r\n.btn {\r\n    background: var(--dark);\r\n    color: var(--light);\r\n    padding: 0.6rem 1.3rem;\r\n    text-decoration: none;\r\n    border: 0;\r\n}\r\n\r\n.main-btn {\r\n    text-decoration: none;\r\n    color: #0b2e13;\r\n}\r\n\r\n.main-btn:hover {\r\n    text-decoration: none;\r\n\r\n}\r\n\r\n\r\nimg {\r\n    max-width: 100%;\r\n}\r\n\r\n.wrapper {\r\n    display: grid;\r\n    grid-gap: 20px;\r\n}\r\n\r\n/* Navigation */\r\n.main-nav ul {\r\n    display: grid;\r\n    grid-gap: 20px;\r\n    padding: 0;\r\n    list-style: none;\r\n    grid-template-columns: repeat(4, 1fr);\r\n}\r\n\r\n.main-nav a {\r\n    background: var(--primary);\r\n    display: block;\r\n    text-decoration: none;\r\n    padding: 0.8rem;\r\n    text-align: center;\r\n    color: var(--dark);\r\n    text-transform: uppercase;\r\n    font-size: 1.1rem;\r\n    box-shadow: var(--shadow);\r\n}\r\n\r\n.main-nav a:hover {\r\n    background: var(--dark);\r\n    color: var(--light);\r\n}\r\n\r\n/* Top Container */\r\n.top-container {\r\n    display: grid;\r\n    grid-gap: 20px;\r\n    grid-template-areas:\r\n    'showcase showcase top-box-a'\r\n    'showcase showcase top-box-b';\r\n}\r\n\r\n/* Showcase */\r\n.showcase {\r\n    grid-area: showcase;\r\n    min-height: 400px;\r\n    background: url('/images/amazing-routes.jpg');\r\n    background-size: cover;\r\n    background-position: center;\r\n    padding: 3rem;\